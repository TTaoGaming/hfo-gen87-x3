{
	"name": "hfo-gen87-x3",
	"version": "87.3.0",
	"description": "HFO Gen87.X3 - Pre-configured AI Workspace with Enforcement",
	"type": "module",
	"main": "dist/index.js",
	"scripts": {
		"build": "tsc",
		"build:bundle": "tsx scripts/build-browser-bundle.ts",
		"dev": "tsx watch src/index.ts",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:changed": "vitest run --changed",
		"test:bucket:contracts": "vitest run sandbox/src/contracts/",
		"test:bucket:adapters": "vitest run sandbox/src/adapters/",
		"test:bucket:smoothers": "vitest run sandbox/src/smoothers/",
		"test:bucket:phase1": "vitest run sandbox/src/phase1-w3c-cursor/",
		"test:bucket:gesture": "vitest run sandbox/src/gesture/ sandbox/src/arming/",
		"test:bucket:status": "tsx scripts/test-bucket-status.ts",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"typecheck": "tsc --noEmit",
		"prepare": "husky",
		"context": "tsx scripts/context-manager.ts generate",
		"context:hive": "tsx scripts/context-manager.ts hive",
		"context:compact": "tsx scripts/context-manager.ts compact",
		"enforce": "tsx scripts/enforce-architecture.ts ./sandbox",
		"enforce:strict": "tsx scripts/enforce-architecture.ts ./sandbox --strict",
		"enforce:json": "tsx scripts/enforce-architecture.ts ./sandbox --json",
		"validate:hive": "tsx scripts/validate-hive.ts",
		"hive:status": "tsx scripts/validate-hive.ts",
		"hive:phase": "tsx -e \"const fs=require('fs');const bb=fs.readFileSync('sandbox/obsidianblackboard.jsonl','utf8').trim().split('\\n');const last=bb.filter(l=>l).reverse().find(l=>JSON.parse(l).hive!=='X');console.log(last?JSON.parse(last).hive:'H');\"",
		"detect:stubs": "tsx scripts/detect-stubs.ts",
		"detect:stubs:json": "tsx scripts/detect-stubs.ts --json",
		"v-phase": "tsx scripts/v-phase-gate.ts",
		"v-phase:staged": "tsx scripts/v-phase-gate.ts --staged",
		"v-phase:demo": "tsx scripts/v-phase-gate.ts sandbox/demo-golden",
		"v-phase:pipeline": "tsx scripts/v-phase-gate.ts sandbox/src/pipeline",
		"detect:theater": "tsx scripts/theater-detector.ts",
		"detect:theater:json": "tsx scripts/theater-detector.ts --json",
		"audit:red-regnant": "tsx scripts/red-regnant-audit.ts",
		"audit:red-regnant:json": "tsx scripts/red-regnant-audit.ts --json",
		"audit:lies": "tsx scripts/red-regnant-v2.ts",
		"audit:lies:json": "tsx scripts/red-regnant-v2.ts --json",
		"audit:pain": "tsx scripts/red-regnant-v2.ts",
		"audit:ttao": "tsx scripts/red-regnant-v2.ts",
		"mutate": "stryker run",
		"mutate:dry": "stryker run --dryRunOnly",
		"mutate:incremental": "stryker run --incremental",
		"mutate:sample": "stryker run --mutate sandbox/src/smoothers/*.ts",
		"adversarial": "tsx scripts/adversarial-red-regnant.ts",
		"adversarial:evolve": "tsx scripts/adversarial-red-regnant.ts --evolve",
		"gate:theater": "tsx scripts/anti-theater-gate.ts",
		"gate:theater:json": "tsx scripts/anti-theater-gate.ts --json",
		"validate:demo": "tsx scripts/validate-demo.ts",
		"validate:demo:all": "tsx scripts/validate-demo.ts --all",
		"regression:check": "tsx scripts/regression-detector.ts",
		"audit:trl": "grep -r -l 'z.object' sandbox --include='*.ts' | xargs grep -L '@source\\|@see\\|TRL\\|W3C\\|CNCF'",
		"orchestration:test": "tsx src/orchestration/test-orchestration.ts",
		"temporal:worker": "tsx src/orchestration/temporal.worker.ts",
		"temporal:client": "tsx src/orchestration/temporal.client.ts",
		"crewai:test": "python src/orchestration/crewai_hive.py",
		"dashboard:update": "tsx scripts/update-dashboard.ts",
		"dashboard:open": "start HFO_DEV_DASHBOARD.html",
		"trust:status": "tsx scripts/trust-enforce.ts --status",
		"trust:enforce": "tsx scripts/trust-enforce.ts",
		"trust:check": "tsx scripts/trust-enforce.ts --status",
		"test:reputation": "vitest run src/reputation/"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.0",
		"@commitlint/cli": "^19.0.0",
		"@commitlint/config-conventional": "^19.0.0",
		"@playwright/test": "^1.57.0",
		"@types/node": "^22.0.0",
		"@vitest/coverage-v8": "^2.1.0",
		"esbuild": "^0.27.2",
		"fast-check": "^3.23.2",
		"husky": "^9.0.0",
		"jsdom": "^27.4.0",
		"tsx": "^4.19.0",
		"typescript": "^5.6.0",
		"vitest": "^2.1.0"
	},
	"dependencies": {
		"@dimforge/rapier2d-compat": "^0.19.3",
		"@langchain/core": "^1.1.8",
		"@langchain/langgraph": "^1.0.7",
		"@langchain/openai": "^1.2.0",
		"@mediapipe/tasks-vision": "^0.10.22-rc.20250304",
		"@nats-io/jetstream": "^3.3.0",
		"@nats-io/kv": "^3.3.0",
		"@nats-io/nats-core": "^3.3.0",
		"@nats-io/obj": "^3.3.0",
		"@opentelemetry/api": "^1.9.0",
		"@opentelemetry/sdk-trace-web": "^2.2.0",
		"@temporalio/activity": "^1.14.0",
		"@temporalio/client": "^1.14.0",
		"@temporalio/worker": "^1.14.0",
		"@temporalio/workflow": "^1.14.0",
		"1eurofilter": "^1.2.2",
		"dotenv": "^17.2.3",
		"golden-layout": "^2.6.0",
		"openai": "^6.15.0",
		"pixi.js": "^8.14.3",
		"rxjs": "^7.8.2",
		"xstate": "^5.25.0",
		"zod": "^3.25.76"
	}
}
